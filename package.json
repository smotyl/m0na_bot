{
  "name": "m0na_bot",
  "version": "0.0.1",
  "main": "src/index.js",
  "license": "MIT",
  "repository": "https://github.com/smotyl/m0na_bot",
  "author": "Sandro Motyl",
  "scripts": {
    "start:react": "basic-react-scripts start",
    "build:react": "basic-react-scripts build",
    "dev": "electron ./src/electron/index.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist:win": "electron-builder --win zip --x64",
    "lint": "eslint .",
    "test": "jest .",
    "coverage": "jest . --coverage"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.6",
    "babel-eslint": "^10.1.0",
    "basic-react-scripts": "^0.0.17",
    "electron": "^12.0.6",
    "electron-builder": "^22.10.5",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
